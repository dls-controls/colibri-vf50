# Skeleton required for saved network configuration

d 755 /state/etc/network
d 744 /state/etc/dropbear

# Initial state for use on development network
f 755 /state/etc/network/interfaces state/interfaces
f 755 /state/etc/hostname state/hostname
f 755 /state/etc/ntp.conf state/ntp.conf
f 755 /state/etc/resolv.conf state/resolv.conf
